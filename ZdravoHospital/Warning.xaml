<Window x:Class="ZdravoHospital.Warning"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoHospital"
        mc:Ignorable="d"
        Title="Warning" Height="170" Width="288">
    <Grid Margin="0,0,0,-16">
        <Label x:Name="titleLabel" Content="Warning!" FontSize="16px" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="yesButton" Content="Yes" HorizontalAlignment="Left" Margin="78,94,0,0" VerticalAlignment="Top" Height="23" Width="41" Click="yesClick"/>
        <Button x:Name="noButton" Content="No" HorizontalAlignment="Left" Margin="169,94,0,0" VerticalAlignment="Top" Height="23" Width="41" Click="noButton_Click"/>
        <Label x:Name="questionLabel" MaxWidth="200" Content="Text" FontSize="14px" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,52,0,0"/>

    </Grid>
</Window>
